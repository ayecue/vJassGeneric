library Object requires InstanceHandler
    //! textmacro ObjectTypeBasic takes NAME,TYPE,FUNC,SETGET
    struct $NAME$API extends array
        method has takes string prop returns boolean
            return HaveSaved$FUNC$(Object.hCache, this, StringHash(prop))
        endmethod

        method operator []= takes string prop, $TYPE$ value returns nothing
            call Save$SETGET$(Object.hCache, this, StringHash(prop), value)
        endmethod

        method operator [] takes string prop returns $TYPE$
            return Load$SETGET$(Object.hCache, this, StringHash(prop))
        endmethod

        method remove takes string prop returns nothing
            call RemoveSaved$FUNC$(Object.hCache, this, StringHash(prop))
        endmethod
    endstruct
    private module $NAME$
        method operator $TYPE$ takes nothing returns $NAME$API
            return this
        endmethod
    endmodule
    //! endtextmacro
    
    //! textmacro ObjectTypeHandle takes NAME,TYPE
    struct $NAME$API extends array
        method has takes string prop returns boolean
            return HaveSavedHandle(Object.hCache, this, StringHash(prop))
        endmethod
    
        method operator []= takes string prop, $TYPE$ value returns nothing
            call Save$NAME$Handle(Object.hCache, this, StringHash(prop), value)
        endmethod

        method operator [] takes string prop returns $TYPE$
