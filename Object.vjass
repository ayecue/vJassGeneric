        endmethod
        
        method remove takes string prop returns nothing
            call RemoveSavedHandle(Object.hCache, this, StringHash(prop))
        endmethod
    endstruct
    private module $NAME$
        method operator $TYPE$ takes nothing returns $NAME$API
            return this
        endmethod
    endmodule
