        static hashtable hCache
        
        static method operator new takes nothing returns thistype
            return thistype.allocate()
        endmethod
        
        method operator [] takes string prop returns Object
            return LoadInteger(thistype.hCache, this, StringHash(prop))
        endmethod
    
        method operator []= takes string prop, Object object returns nothing
            call SaveInteger(thistype.hCache, this, StringHash(prop), object)
        endmethod
    
        method has takes string prop returns boolean
            return HaveSavedInteger(thistype.hCache, this, StringHash(prop))
        endmethod
    
        method remove takes string prop returns nothing
            call RemoveSavedInteger(thistype.hCache, this, StringHash(prop))
        endmethod
        
        static method onInit takes nothing returns nothing
            set thistype.hCache = Hashtable.new
        endmethod
    endstruct
    
    //! runtextmacro Array("Object","Object","-1","8190","implement ObjectFactory")
    
    module ObjectFactory
        static method get takes nothing returns Object
            local Object new = Object.new
            
            call thistype.push(new)
            
            return new
        endmethod
    endmodule
